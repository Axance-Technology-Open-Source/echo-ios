{
  "name": "Echo",
  "version": "0.0.1",
  "summary": "Echo: Record & Replay JSON webservices responses easily!",
  "description": "create and use JSON file to mock your app easily!\"\n\n2 modes available:\n- record\n- mock-responses\n\noverride json files on asset folder by following file nomenclature!",
  "homepage": "https://github.com/Axance-Technology-Open-Source/echo-ios",
  "license": {
    "type": "MIT",
    "file": "License.md"
  },
  "authors": {
    "Charles Legrand": "charles.legrand@devoteam.com"
  },
  "source": {
    "git": "https://github.com/Axance-Technology-Open-Source/echo-ios.git",
    "tag": "0.0.1"
  },
  "swift_versions": "5",
  "platforms": {
    "ios": "8.0"
  },
  "source_files": "Echo/*.{swift}",
  "default_subspecs": "Moya13",
  "subspecs": [
    {
      "name": "Moya14-beta",
      "dependencies": {
        "Moya/Core": [
          "~> 14.0.0-beta.0"
        ]
      },
      "source_files": [
        "Echo/*.{swift}",
        "Echo/Moya/*.{swift}"
      ],
      "platforms": {
        "ios": "10.0"
      }
    },
    {
      "name": "Moya13",
      "dependencies": {
        "Moya/Core": [
          "~> 13.0"
        ]
      },
      "source_files": [
        "Echo/*.{swift}",
        "Echo/Moya/*.{swift}"
      ]
    },
    {
      "name": "Moya12",
      "dependencies": {
        "Moya/Core": [
          "~> 12.0"
        ]
      },
      "source_files": [
        "Echo/*.{swift}",
        "Echo/Moya/*.{swift}"
      ]
    }
  ],
  "swift_version": "5"
}
